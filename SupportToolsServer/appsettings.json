{
  "Data": {
    "SupportToolsServerDatabase": {
      "ConnectionString": "DatabaseConnectionString"
    }
  },
  "IdentitySettings": {
    "JwtSecret": "JwtSecret"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5033"
      }
    }
  },
  "Logging": {
    "File": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "PathToLogFile",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "VersionInfo": {
    "AppSettingsVersion": "1.1"
  }
}